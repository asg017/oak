a = cell({
    path: 'temp/a.txt',
    recipe: a => bash`echo "hello its a! " > ${a}`
})


b = cell({
    path: 'temp/b.txt',
    recipe: b => bash`echo "hello now its b! " > ${b}`
})

c = cell({
    path: 'temp/c.txt',
    recipe: c => bash`cat ${a} ${b} > ${c}`
})

d = cell({
    path: 'temp/d.txt',
    recipe: d=> bash`cat ${c} > ${d}`
})

e = cell({
    path: 'temp/e.txt',
    recipe: e=> bash`cat ${d} > ${e}`
})

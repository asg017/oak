{
  "name": "@alex.garcia/oak",
  "version": "0.0.14",
  "description": "Easy, simple, data workflows",
  "main": "./built/cli.js",
  "bin": {
    "oak": "./built/cli.js"
  },
  "files": [
    "built/**/*"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asg017/oak.git"
  },
  "keywords": [
    "etl",
    "pipeline",
    "workflow",
    "cli",
    "dag",
    "task",
    "oak"
  ],
  "scripts": {
    "build": "tsc && npm run build-site ",
    "build-site": "cp -r ./src/commands/dash/dash-frontend/dist ./built/commands/dash/dash-frontend/",
    "prepare": "rm -rf built && npm run build",
    "preversion": "npm test",
    "version": "rm -rf built && npm run build",
    "postversion": "git push && git push --tags && rm -rf build/temp",
    "format": "prettier --write src/**/*.ts",
    "format-test": "prettier --check src/**/*.ts",
    "test": "npm run format-test && ts-node ./node_modules/tape/bin/tape tests/**/*.ts",
    "t": "ts-node ./node_modules/tape/bin/tape",
    "o": "ts-node ./src/cli.ts"
  },
  "dependencies": {
    "@microsoft/ts-command-line": "^4.2.6",
    "@observablehq/parser": "^1.2.1",
    "@observablehq/runtime": "4.4.4",
    "@observablehq/stdlib": "^3.0.2",
    "@types/node": "^12.6.8",
    "@types/tape": "^4.2.33",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "d3-array": "^2.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fs-extra": "^8.1.0",
    "graphviz": "^0.0.9",
    "hasha": "^5.1.0",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.1",
    "randomatic": "^3.1.1",
    "read-package-json": "^2.1.0",
    "socket.io": "^2.2.0",
    "socketio-jwt": "^4.5.0",
    "untildify": "^4.0.0",
    "yn": "^3.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.1.0",
    "@types/graphviz": "^0.0.30",
    "@types/pino": "^5.17.0",
    "mock-fs": "^4.10.1",
    "prettier": "^1.19.1",
    "surge": "^0.21.3",
    "tape": "^4.11.0",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  }
}

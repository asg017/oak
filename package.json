{
  "name": "@alex.garcia/oak",
  "version": "0.0.19",
  "description": "Easy, simple, data workflows",
  "main": "./built/cli.js",
  "bin": {
    "oak": "./built/cli.js"
  },
  "files": [
    "built/**/*"
  ],
  "pkg": {
    "assets": [
      "built/commands/studio/frontend/dist",
      "built/commands/schedule/dashboard/dist"
    ],
    "targets": [
      "node10.17-linux-x64",
      "node10.17-alpine-x64",
      "node10.17-macos-x64",
      "node10.17-win-x64"
    ]
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asg017/oak.git"
  },
  "keywords": [
    "etl",
    "pipeline",
    "workflow",
    "cli",
    "dag",
    "task",
    "oak"
  ],
  "scripts": {
    "build": "tsc && npm run build-studio && npm run build-schedule-dash",
    "build-studio": "mkdir -p ./built/commands/studio/frontend/dist/ && cp -r ./src/commands/studio/frontend/dist ./built/commands/studio/frontend/",
    "build-schedule-dash": "mkdir -p ./built/commands/schedule/dashboard/dist/ && cp -r ./src/commands/schedule/dashboard/dist/ ./built/commands/schedule/dashboard/",
    "pkg": "mkdir -p pkg && pkg . --out-path ./pkg",
    "prepare": "rm -rf built && npm run build",
    "preversion": "npm test",
    "version": "rm -rf built && npm run build",
    "postversion": "git push && git push --tags && rm -rf build/temp",
    "format": "prettier --write src/**/*.ts",
    "format-test": "prettier --check src/**/*.ts",
    "test": "npm run format-test && ts-node ./node_modules/tape/bin/tape tests/**/*.ts",
    "t": "ts-node ./node_modules/tape/bin/tape",
    "o": "ts-node ./src/cli.ts"
  },
  "dependencies": {
    "@observablehq/parser": "^4.0.1",
    "@observablehq/runtime": "4.4.4",
    "@observablehq/stdlib": "^3.0.2",
    "@rushstack/ts-command-line": "^4.3.13",
    "better-sqlite3": "^7.0.1",
    "chalk": "^2.4.2",
    "chokidar": "^3.3.1",
    "cors": "^2.8.5",
    "cronstrue": "^1.92.0",
    "d3-array": "^2.2.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "hasha": "^5.1.0",
    "immutable": "^4.0.0-rc.12",
    "ink": "^3.0.4",
    "ink-spinner": "4.0.1",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.1",
    "react": "^16.13.1",
    "read-package-json": "^2.1.0",
    "socket.io": "^2.3.0",
    "split2": "^3.1.1",
    "untildify": "^4.0.0",
    "yn": "^3.1.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.1.0",
    "@types/graphviz": "^0.0.30",
    "@types/node": "^13.9.5",
    "@types/pino": "^5.17.0",
    "@types/react": "^16.9.32",
    "@types/socket.io": "^2.1.4",
    "@types/split2": "^2.1.6",
    "@types/tape": "^4.2.34",
    "mock-fs": "^4.10.1",
    "pkg": "^4.4.7",
    "prettier": "^1.19.1",
    "surge": "^0.21.3",
    "tape": "^4.11.0",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  }
}
